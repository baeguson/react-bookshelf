{
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "quotes": [1, "single"],
    "prettier/prettier": ["warn", {"singleQuote": true, "parser": "flow"}],
    "strict": 0,
    "new-cap": [2, { "capIsNewExceptions": ["List", "Map", "Set"] }]
  },
  "extends": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [1, "single"]
  }
}
